<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
      	
      	<!--
      	  ==========================================
      	    Autostart the plug-in
      	  ==========================================
      	-->

   <extension
         point="org.eclipse.ui.startup">
      <startup class="biz.isphere.rexec.StartUp"/>
   </extension>
      	
      	<!--
      	  ==========================================
      	    iSphere Preferences
      	  ==========================================
      	-->

   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="biz.isphere.rexec.preferencepages.ISphereRexec"
            category="biz.isphere.base.preferencepages.General"
            name="%Rexec_Server"
            id="biz.isphere.rexec.preferencepages.ISphereRexec"/>
   </extension>
   
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="biz.isphere.rexec.preferences.PreferencesInitializer">
      </initializer>
   </extension>

</plugin>
